{
    "name": "zenstruck/assert-html",
    "description": "Fluent html assertions plugin for zenstruck/assert.",
    "homepage": "https://github.com/zenstruck/assert-html",
    "type": "library",
    "license": "MIT",
    "keywords": ["test", "assertion", "phpunit"],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "behat/mink": "^1.10",
        "symfony/dom-crawler": "^5.4|^6.0",
        "zenstruck/assert": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.0",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Assert\\": ["src/"] }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Assert\\Tests\\": ["tests/"] }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
